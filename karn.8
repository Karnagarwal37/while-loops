//while loops
#include<stdio.h>
int main(){
    int i=1;
    while(i<=5){
        printf("%d\n",i);
        i++;
    }
}

//example on while
#include<stdio.h>
int main(){
    int n;
    printf("enter the no :");
    scanf("%d",&n);
    int i=0;
    while(i<=n){
        printf("%d\n",i);
        i++;
    }
}
